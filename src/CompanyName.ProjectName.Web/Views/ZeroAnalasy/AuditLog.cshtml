@{
    ViewBag.Title = "AuditLog";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ActiveMenu = ZeroPageNames.AuditLog;
}
<nav aria-label="breadcrumb">
    <ol class="breadcrumb adbp-breadcrumb-background">
        <li class="breadcrumb-item"><a href="/">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">AuditLog</li>
    </ol>
</nav>
<div class="card adbp-background">
    <div class="card-body">
        <div class="bd-highlight adbp-toolbar">
            <div class="float-right">
                <div class="input-group">
                    <input id="table-search" type="text" class="form-control adbp-typeahead" data-suggestions="@(Symbol)exception" placeholder="search" value="@(Symbol)exception">
                </div>
            </div>
        </div>
        <table id="auditLog-table" class="display">
            <thead>
                <tr>
                    <th>@L("OccurrenceTime")</th>
                    <th>@L("Operator")</th>
                    <th>@L("Service")</th>
                    <th>@L("Method")</th>
                    <th>@L("Params")</th>
                    <th>@L("Duration")</th>
                    <th>@L("IpAddress")</th>
                    <th>@L("Client")</th>
                    <th>@L("Browser")</th>
                    <th>@L("Exception")</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
@section scripts {
    @Html.IncludeScript("~/wwwroot/es5/ZeroAnalasy/AuditLog.js")
}